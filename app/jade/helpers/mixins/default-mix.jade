mixin objCreate(keys, vals)
	-var arr = []
	each key, i in keys
		each val, j in vals
			if i === j
				-obj='{'+key+': '+val+'}'
				-arr.push(obj)
				
				
				
mixin textRow(items)
	each item, i in items
		=item
		if i < (items.length-1)
			br

mixin actionDescText(items)
	-var arr = []
	each item in items
		-var part=item.name + ': ' + item.value
		-arr.push(part)
	+textRow(arr)